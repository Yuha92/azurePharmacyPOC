{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "yuhaADFpoc"
		},
		"AzureFunctionConn_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'AzureFunctionConn'"
		},
		"HDIClusterconn_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'HDIClusterconn'"
		},
		"SparkClusterConn_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SparkClusterConn'"
		},
		"pharmacypocblobconn_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'pharmacypocblobconn'"
		},
		"HDIClusterconn_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin"
		},
		"SparkClusterConn_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin"
		},
		"keyvaultPOCconn_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://yuhaKeyVaultpoc.vault.azure.net/"
		},
		"pharmacypocADLSconn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://yuhaadlspoc.dfs.core.windows.net/"
		},
		"casesTrigger_properties_Stage1_parameters_FileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"casesTrigger_properties_Stage1_parameters_FilePath": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"casesTrigger_properties_Stage1_parameters_ParamFilePath": {
			"type": "string",
			"defaultValue": "pharmacypoc/params"
		},
		"casesTrigger_properties_Stage1_parameters_ParamFileName": {
			"type": "string",
			"defaultValue": "cases_params.csv"
		},
		"casesTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4252b742-3cd4-47a1-af43-30bb66d77529/resourceGroups/yuhaPOC/providers/Microsoft.Storage/storageAccounts/yuhaadlspoc"
		},
		"curationcasestrigger_properties_Stage2_parameters_ParamPath": {
			"type": "string",
			"defaultValue": "pharmacypoc/params/curation"
		},
		"curationcasestrigger_properties_Stage2_parameters_ParamName": {
			"type": "string",
			"defaultValue": "cases_params.csv"
		},
		"curationcasestrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4252b742-3cd4-47a1-af43-30bb66d77529/resourceGroups/yuhaPOC/providers/Microsoft.Storage/storageAccounts/yuhaadlspoc"
		},
		"deathsTrigger_properties_Stage1_parameters_FileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"deathsTrigger_properties_Stage1_parameters_FilePath": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"deathsTrigger_properties_Stage1_parameters_ParamFilePath": {
			"type": "string",
			"defaultValue": "pharmacypoc/params"
		},
		"deathsTrigger_properties_Stage1_parameters_ParamFileName": {
			"type": "string",
			"defaultValue": "deaths_params.csv"
		},
		"deathsTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4252b742-3cd4-47a1-af43-30bb66d77529/resourceGroups/yuhaPOC/providers/Microsoft.Storage/storageAccounts/yuhaadlspoc"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureFunctionConn_functionKey": {
						"value": "[parameters('AzureFunctionConn_functionKey')]"
					},
					"pharmacypocblobconn_connectionString": {
						"value": "[parameters('pharmacypocblobconn_connectionString')]"
					},
					"HDIClusterconn_password": {
						"value": "[parameters('HDIClusterconn_password')]"
					},
					"SparkClusterConn_password": {
						"value": "[parameters('SparkClusterConn_password')]"
					},
					"keyvaultPOCconn_properties_typeProperties_baseUrl": {
						"value": "[parameters('keyvaultPOCconn_properties_typeProperties_baseUrl')]"
					},
					"pharmacypocADLSconn_properties_typeProperties_url": {
						"value": "[parameters('pharmacypocADLSconn_properties_typeProperties_url')]"
					},
					"HDIClusterconn_properties_typeProperties_userName": {
						"value": "[parameters('HDIClusterconn_properties_typeProperties_userName')]"
					},
					"SparkClusterConn_properties_typeProperties_userName": {
						"value": "[parameters('SparkClusterConn_properties_typeProperties_userName')]"
					},
					"casesTrigger_properties_Stage1_parameters_FileName": {
						"value": "[parameters('casesTrigger_properties_Stage1_parameters_FileName')]"
					},
					"casesTrigger_properties_Stage1_parameters_FilePath": {
						"value": "[parameters('casesTrigger_properties_Stage1_parameters_FilePath')]"
					},
					"casesTrigger_properties_Stage1_parameters_ParamFilePath": {
						"value": "[parameters('casesTrigger_properties_Stage1_parameters_ParamFilePath')]"
					},
					"casesTrigger_properties_Stage1_parameters_ParamFileName": {
						"value": "[parameters('casesTrigger_properties_Stage1_parameters_ParamFileName')]"
					},
					"casesTrigger_properties_typeProperties_scope": {
						"value": "[parameters('casesTrigger_properties_typeProperties_scope')]"
					},
					"curationcasestrigger_properties_Stage2_parameters_ParamPath": {
						"value": "[parameters('curationcasestrigger_properties_Stage2_parameters_ParamPath')]"
					},
					"curationcasestrigger_properties_Stage2_parameters_ParamName": {
						"value": "[parameters('curationcasestrigger_properties_Stage2_parameters_ParamName')]"
					},
					"curationcasestrigger_properties_typeProperties_scope": {
						"value": "[parameters('curationcasestrigger_properties_typeProperties_scope')]"
					},
					"deathsTrigger_properties_Stage1_parameters_FileName": {
						"value": "[parameters('deathsTrigger_properties_Stage1_parameters_FileName')]"
					},
					"deathsTrigger_properties_Stage1_parameters_FilePath": {
						"value": "[parameters('deathsTrigger_properties_Stage1_parameters_FilePath')]"
					},
					"deathsTrigger_properties_Stage1_parameters_ParamFilePath": {
						"value": "[parameters('deathsTrigger_properties_Stage1_parameters_ParamFilePath')]"
					},
					"deathsTrigger_properties_Stage1_parameters_ParamFileName": {
						"value": "[parameters('deathsTrigger_properties_Stage1_parameters_ParamFileName')]"
					},
					"deathsTrigger_properties_typeProperties_scope": {
						"value": "[parameters('deathsTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}